  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "conventional-alberta",
   "metadata": {},
   "outputs": [],
   "source": [
    "#get data\n",
    "x = np.array([])\n",
    "y = np.array([])\n",
    "\n",
    "data = open('data.txt','r')\n",
    "for i in data.readlines():\n",
    "    data2 = i.split(',')\n",
